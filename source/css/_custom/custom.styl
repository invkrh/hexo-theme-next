// Custom styles.
.katex-display .katex-html {
    overflow-x: auto;
    overflow-y: hidden;
    white-space: nowrap;
    -webkit-overflow-scrolling: touch;
}

ul, ol {
    padding-inline-start: 20px;
}

// Make bullet items block close to preceded paragraph
p + ul, p + ol {
    margin-top: -1em;
}
